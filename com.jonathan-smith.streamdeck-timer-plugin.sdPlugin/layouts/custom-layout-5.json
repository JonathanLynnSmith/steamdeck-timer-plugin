{
	"$schema": "https://schemas.elgato.com/streamdeck/plugins/layout.json",
	"id": "custom-layout-3",
	"items": [
		{
			"key": "time",
			"type": "text",
			"font": { "size": 32, "weight": 600 },
			"rect": [25, 6, 150, 50]
		},
		{
			"key": "progress",
			"type": "bar",
			"range": { "min": 0, "max": 100 },
			"rect": [25, 66, 150, 20],
			"value": 50,
			"subtype": 4,
			"border_w": 1,
			"bar_bg_c": "#00000000", 
			"bar_fill_c": "#FFFFFF"
		}
	]
}